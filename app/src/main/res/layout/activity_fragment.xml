<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.FragmentPageActivity">

    <Button
        android:id="@+id/ax_fragment_btn"
        android:layout_width="@dimen/activity_button_width"
        android:layout_height="wrap_content"
        android:text="@string/activity_fragment_ax"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/navigation_fragment_btn"
        android:layout_width="@dimen/activity_button_width"
        android:layout_height="wrap_content"
        android:text="@string/activity_fragment_navigation"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/ax_fragment_btn"/>

</androidx.constraintlayout.widget.ConstraintLayout>